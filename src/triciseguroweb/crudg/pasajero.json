{
    "model_name":"Pasajero",
    "soft_deletes":"yes",
    "pk":"id",
    "localize":"yes",
    "table":"pasajeros",
    "fields": [
        {
            "name": "nombre",
            "type": "string"
        },
        {
            "name": "contacto_id",
            "type": "integer#unsigned"
        },   
        {
            "name": "solicitud_id",
            "type": "integer#unsigned"
        }
         
         
    ],
      "foreign_keys": [
        {
            "column": "contactos_id",
            "references": "id",
            "on": "contactos",
            "onDelete": "cascade"
        },
          
          {
            "column": "solicitud_id",
            "references": "id",
            "on": "solicitud",
            "onDelete": "cascade"
        }
          
          
    ],
    "relationships": [
        {
            "name": "contacto",
            "type": "belongsTo",
            "class": "App\\Contacto"
        },
       
        {
            "name": "solicitud",
            "type": "belongsTo",
            "class": "App\\Solicitud"
        }
       
       
    ],
    "validations": [
        {
            "field": "nombre",
            "rules": "required|min:5|max:20"
        }
    ]
}
