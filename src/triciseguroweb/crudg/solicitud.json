{
	"model_name":"Solicitud",
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"solicitudes",
    "fields": [
      
       
        
        {
            "name": "pasajero_id",
            "type": "integer#unsigned"
        },
        {
            "name":"ruta_id",
            "type":"integer#unsigned"
        },
        
        {
            "name":"calificacion_id",
            "type":"integer#unsigned"
        },
        {
            "name":"taxista_id",
            "type":"integer#unsigned"
        }
       
      
    ],
    
    "foreign_keys": [
      
        {
            "column": "pasajero_id",
            "references": "id",
            "on": "pasajeros",
            "onDelete": "cascade"
        },
        {
            "column": "ruta_id",
            "references": "id",
            "on": "rutas",
            "onDelete": "cascade"
        },
        {
            "column": "calificacion_id",
            "references": "id",
            "on": "calificaciones",
            "onDelete": "cascade"
        },
        {
            "column": "taxista_id",
            "references": "id",
            "on": "taxistas",
            "onDelete": "cascade"
        }
       
    ],

    "relationships": [

        {
            "name": "ruta",
            "type": "hasOne",
            "class": "App\\Ruta"
        },
        {
            "name": "calificacion",
            "type": "hasOne",
            "class": "App\\Calificacion"
        },
        {
            "name": "mensajes",
            "type": "belongsTo",
            "class": "App\\Mensaje"
        },
        {
            "name": "taxista",
            "type": "hasOne",
            "class": "App\\Taxista"
        },
        {
            "name": "pasajero",
            "type": "hasOne",
            "class": "App\\Pasajero"
        },
        {
            "name": "servicio",
            "type": "belongsTo",
            "class": "App\\Servicio"
        },
        {
            "name": "viaje",
            "type": "belongsTo",
            "class": "App\\Viaje"
        }     

    ],
    
    "validations": [
        {
            "field": "pasajero_id", 
            "rules": "required|min:5"
        },
		{
            "field": "ruta_id",
            "rules": "required|min:5"
        },
        {
            "field": "calificacion_id",
            "rules": "required|min:5"
        },
        {
            "field": "taxista_id",
            "rules": "required|min:5"
        }
    ]
}
