{
	"model_name":"Comprobante_Pago",
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"comprobante_pagos",
    "fields": [  {
            "name": "cantidad",
            "type": "integer"
        },
        {
            "name": "descripcion",
            "type": "text"
        },
        

        {
            "name": "documento_id",
            "type": "integer#unsigned"
        }
    ],
    "foreign_keys": [
        {
            "column": "documento_id",
            "references": "id",
            "on": "documento",
            "onDelete": "cascade"
        }
       ],
    "relationships": [
         {
            "name": "documento",
            "type": "hasOne",
            "class": "App\\Models\\Documento"
        }

    ],
    "validations": [
        {
            "field": "cantidad",
            "rules": "required|min:5|max:20"
        }
    ]
}