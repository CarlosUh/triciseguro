{
	"model_name":"Documentos",
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"documentos",
    "fields": [	
    {
            "name": "nombre",
            "type": "string"
        },
        {
            "name": "apellido",
            "type": "string"
        },
        {
            "name": "curp",
            "type": "string"
        },
        {
            "name": "fecha",
            "type": "string"
        },
        {
            "name": "taxista_id",
            "type": "integer#unsigned"
        }
    ],
    "foreign_keys": [
        {
            "column": "taxista_id",
            "references": "id",
            "on": "credencial",
            "onDelete": "cascade"
        }
    ],
    "relationships": [
        {
            "name": "taxista_id",
            "type": "hasMany",
            "class": "App\\Models\\Taxista"
        }
     ],  
    "validations": [
        {
            "field": "nombre",
            "rules": "required|min:5|max:20"
        },
        {
            "field": "apellido",
            "rules": "required|min:20"
        },
        {
            "field": "curp",
            "rules": "required|min:5"
        }
        ,
        {
            "field": "fecha",
            "rules": "required|min:6"
        }
        ,
        {
            "field": "taxista_id",
            "rules": "required|min:20"
        }
    ]
	
}