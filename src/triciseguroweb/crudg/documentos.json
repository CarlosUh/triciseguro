{
	"model_name":"Documentos",
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"documentos",
    "fields": [		
        {
            "name": "credencial_id",
            "type": "integer#unsigned"
        },
        {
            "name": "cuota_id",
            "type": "integer#unsigned"
        },
        {
            "name": "licencia_id",
            "type": "integer#unsigned"
        },
        {
            "name": "comprobante_pago_id",
            "type": "integer#unsigned"
        }
    ],
    "foreign_keys": [
        {
            "column": "credencial_id",
            "references": "id",
            "on": "credencial",
            "onDelete": "cascade"
        },
        {
            "column": "cuota_id",
            "references": "id",
            "on": "cuota",
            "onDelete": "cascade"
        },
        {
            "column": "licencia",
            "references": "id",
            "on": "licencia",
            "onDelete": "cascade"
        },
        {
            "column": "comprobante_pago",
            "references": "id",
            "on": "comprobante_pago",
            "onDelete": "cascade"
        }
    ],
    "relationships": [
        {
            "name": "credencial",
            "type": "belongsTo",
            "class": "App\\Models\\Credencial"
        },
        {
            "name": "cuota",
            "type": "belongsTo",
            "class": "App\\Models\\Cuota"
        },
        {
            "name": "licencia",
            "type": "belongsTo",
            "class": "App\\Models\\Licencia"
        },
        {
            "name": "comprobante_pago",
            "type": "belongsTo",
            "class": "App\\Models\\Comprobante_Pago"
        }
    ],
    "validations": [
        {
            "field": "credencial_id",
            "rules": "required|min:5|max:20"
        },
		{
            "field": "cuota_id",
            "rules": "required|min:5"
        },
        {
            "field": "comprobante_pago_id",
            "rules": "required|min:5"
        },
        {
            "field": "licencia_id",
            "rules": "required|min:5"
        }
    ]
	
}
