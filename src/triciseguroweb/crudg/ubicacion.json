{
	"model_name":"Ubicacion",
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"ubicacion",
    "fields": [		
        {
            "name": "nombre",
            "type": "string"
        },
        {

            "name": "latitude",
            "type": "decimal"

        },
        {

            "name": "longitud",
            "type": "decimal"

        },
        {
            "name": "description",
            "type": "text"
        },
        
        {
            "name": "persona_id",
            "type": "integer#unsigned"
        }
    ],
    "foreign_keys": [
       
        {
            "column": "persona_id",
            "references": "id",
            "on": "persona",
            "onDelete": "cascade"
        }
    ],
    "relationships": [
        
        {
            "name": "persona",
            "type": "belongsTo",
            "class": "App\\Models\\persona"
        }


    ],
    "validations": [
        {
            "field": "name",
            "rules": "required|min:5|max:20"
        },
		{
            "field": "description",
            "rules": "required|min:5"
        }
    ]
	
}