{
	"model_name":"Notificacion",
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"notificacions",
    "fields": [	
        {
            "name": "tipo",
            "type": "string"
        },
        {
            "name": "mensaje",
            "type": "text"
        },
        {
            "name": "origen",
            "type": "integer#unsigned"
        },
        {
            "name": "destino",
            "type": "integer#unsigned"
        }
    
    ],
    "foreign_keys": [
        {
            "column": "origen",
            "references": "id",
            "on": "personas",
            "onDelete": "cascade"
        },
        {
            "column": "destino",
            "references": "id",
            "on": "personas",
            "onDelete": "cascade"
        }
       
    ],
    "relationships": [
        {
            "name": "origen",
            "type": "belongsTo",
            "class": "App\\Models\\Persona"
        },
        {
            "name": "destino",
            "type": "belongsTo",
            "class": "App\\Models\\Persona"
        }
    ],
    "validations": [
        {
            "field": "persona_id",
            "rules": "required|min:5|max:20"
        }
    ]
	
}