        },
				{
            "column": "destino_id",
            "references": "id",
            "on": "personas",
            "onDelete": "cascade"
        }
    ],
    "relationships": [
				
				{
            "name": "origen",
            "type": "belongsTo",
            "class": "App\\Persona"
        },
				{
            "name": "destino",
            "type": "belongsTo",
            "class": "App\\Persona"
        }

    ]

}
