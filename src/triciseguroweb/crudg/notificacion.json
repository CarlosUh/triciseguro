{
	"model_name":"Notificacion",
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"notificacions",
    "fields": [
        {
			"name":"tipo",
			"type":"string"
		},

		{
			"name":"mensaje",
            "type":"text"
		},
			{
          "name": "origen_id",
